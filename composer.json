{
    "name":        "arcanesoft/auth",
    "description": "Authorization package for ARCANESOFT/Foundation.",
    "homepage":    "https://github.com/ARCANESOFT/Auth",
    "keywords":    [
        "arcanesoft", "arcanedev", "auth", "acl", "authorisation", "authorization", "users", "roles", "permissions"
    ],
    "authors":  [
        {
            "name":     "ARCANEDEV",
            "email":    "arcanedev.maroc@gmail.com",
            "homepage": "https://github.com/arcanedev-maroc",
            "role":     "Developer"
        }
    ],
    "type":    "library",
    "license": "MIT",
    "require": {
        "php":                          ">=5.6.4",
        "arcanedev/gravatar":           "~2.0",
        "arcanedev/laravel-auth":       "~2.0",
        "arcanedev/laravel-api-helper": "~1.0",
        "arcanesoft/core":              "~2.0"
    },
    "require-dev": {
        "phpunit/phpcov":    "~3.0",
        "phpunit/phpunit":   "~5.0",
        "laravel/socialite": "~3.0"
    },
    "autoload": {
        "psr-4": {
            "Arcanesoft\\Auth\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Arcanesoft\\Auth\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "testbench": "composer require --dev \"orchestra/testbench=~3.4.0\""
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
