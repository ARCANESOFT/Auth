{
    "name":        "arcanesoft/auth",
    "description": "Auth module for ARCANESOFT.",
    "keywords":    ["arcanedev", "arcanesoft", "auth", "framework", "laravel"],
    "license":     "MIT",
    "type":        "project",
    "authors":     [
        {
            "name":     "ARCANEDEV",
            "email":    "arcanedev.maroc@gmail.com",
            "homepage": "https://github.com/arcanedev-maroc",
            "role":     "Developer"
        }
    ],
    "require": {
        "php": ">=7.2.0",
        "arcanedev/gravatar": "^3.0",
        "arcanedev/laravel-impersonator": "^2.0",
        "arcanesoft/foundation": "@dev",
        "yajra/laravel-datatables-fractal": "^1.5",
        "yajra/laravel-datatables-oracle": "^9.7"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0",
        "phpunit/phpcov":  "^6.0"
    },
    "autoload": {
        "psr-4": {
            "Arcanesoft\\Auth\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Arcanesoft\\Auth\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Arcanesoft\\Auth\\AuthServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    }
}
